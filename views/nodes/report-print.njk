{% set pageTitle = 'Отчет - База «Южная»' %}
{% set pageDescription = 'АО “База «Южная»” Отчет данных аренды' %}

{% extends "layouts/empty.njk" %}

{% block customCss %}

<style>
    .report {
        padding: 50px;
        max-width: 800px;
        margin: 0 auto;
        color: #1C1D20;
    }

    .report__header {
        display: table;
        width: 100%;
        margin-bottom: 40px;
    }

    .report__title, .report__date {
        display: table-cell;
        vertical-align: top;
    }

    .report__title {
        width: 70%;
    }

    .report__date {
        width: 30%;
        text-align: right;
        font-size: 14px;
    }

    .report__h1 {
        font-size: 24px;
        margin-bottom: 5px;
    }

    .report__h2 {
        font-size: 20px;
        text-transform: uppercase;
    }

    .report__h3 {
        font-size: 20px;
        font-weight: 500;
        margin-bottom: 10px;
    }

    .report__h4 {
        font-size: 18px;
        font-weight: 500;
        width: 100%;
        margin-bottom: 5px;
    }

    .report__title {
        margin-right: 20px;
    }

    .report__summary,
    .report__ratio {
        margin-bottom: 20px;
    }

    .report__icon {
        display: inline-block;
        width: 30px;
        vertical-align: middle;
        margin-right: 10px;
    }

    .report__metrics {
        display: table;
        width: 100%;
        margin: 0 -10px;
    }

    .report__metrics--sb {
        justify-content: space-between;
    }

    .report__metric {
        display: table-cell;
        font-weight: 700;
        font-size: 14px;
        padding: 5px 10px;
        width: 33%;
    }

    .report__metric--green {
        color: #5EC7B1;
    }

    .report__metric--red {
        color: #D40000;
    }

    .report__metric--large {
        font-size: 18px;
    }

    .report__metric--thin {
        font-weight: 500;
    }

    .report__metric--right {
        text-align: right;
    }

    .report__metric p:first-child {
        margin-bottom: 5px;
    }

    .report__block {
        max-width: 500px;
        margin-bottom: 15px;
    }

    .report__bar {
        display: table;
        width: 100%;
        border-radius: 10px;
        overflow: hidden;
    }

    .report__percentage {
        display: table-cell;
        max-width: 90%;
        min-width: 20%;
        padding: 10px 20px;
        text-align: center;
    }

    .report__percentage--yellow {
        background-color: #F9BC07;
    }

    .report__percentage--grey {
        background-color: #EFEFEF;
    }

    .report__percentage--green {
        background-color: #5EC7B1;
    }

    .report__percentage--red {
        background-color: #D40000;
        color: white;
    }
</style>

{% endblock %}

