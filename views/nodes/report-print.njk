{% block mainContent %}

<section class="section" id="report">
    <div class="report">
        <div class="report__header">
            <div class="report__title">
                <h1 class="report__h1">АО “Депо”</h1>
                <h2 class="report__h2">Отчет данных аренды</h2>
            </div>
            <div class="report__date">
                <p>Дата формирования отчета: <br /> {{ generatedAt }}</p>
            </div>
        </div>

        <div class="report__summary">
            <h3 class="report__h3">Общие показатели</h3>
            <div class="report__metrics">
                <div class="report__metric">
                    <p>Всего помещений:</p>
                    <p class="report__metric--large">{{ aggregatedData.total }}</p>
                </div>
                <div class="report__metric report__metric--green">
                    <p>В аренде: {{ aggregatedData.rented }}</p>
                    <p class="report__metric--large">{{ aggregatedData.rented_percentage }}%</p>
                </div>
                <div class="report__metric report__metric--red">
                    <p>Свободно: {{ aggregatedData.available }}</p>
                    <p class="report__metric--large">{{ aggregatedData.available_percentage }}%</p>
                </div>
            </div>
        </div>

        <div class="report__ratio">
            <h3 class="report__h3">Соотношение по типам помещений от общего числа</h3>
            <div class="report__metrics">
                {% for type in types %}
                    <div class="report__metric">
                        <p>{{ type.type }}</p>
                        <p class="report__metric--large">{{ type.type_percentage }}%</p>
                    </div>
                {% endfor %}
            </div>
        </div>

        <div class="report__status">
            <h3 class="report__h3">Статус по типам</h3>
            {% for type in types %}
                <div class="report__block">
                    <h4 class="report__h4">
                        <div class="report__icon">
                            {% if type.type == 'Торговое' %} {{ CommercialPremises | safe }}
                            {% elif type.type == 'Холодный склад' %} {{ RefrigeratorPremises | safe }}
                            {% elif type.type == 'Производственно-складское' %} {{ WarehousePremises | safe }}
                            {% elif type.type == 'Офис' %} {{ OfficePremises | safe }}
                            {% elif type.type == 'Участок' %} {{ SitePremises | safe }}
                            {% endif %}
                        </div>
                        <span>{{ type.type }}</span>
                    </h4>
                    <div class="report__bar">
                        <div class="report__percentage 
                            {% if type.rented == type.total %}report__percentage--green
                            {% elseif type.rented == 0 %}report__percentage--red
                            {% else %}report__percentage--yellow
                            {% endif %}" 
                            style="width: {{ type.rented_percentage }}%;">
                            {{ type.rented_percentage }}%
                        </div>

                        <div class="report__percentage 
                            {% if type.available == type.total %}report__percentage--red
                            {% elseif type.available == 0 %}report__percentage--green
                            {% else %}report__percentage--grey
                            {% endif %}" 
                            style="width: {{ type.available_percentage }}%;">
                            {{ type.available_percentage }}%
                        </div>
                    </div>
                    <div class="report__metrics report__metrics--sb">
                        <div class="report__metric">
                            <p>В Аренде</p>
                            <p>{{ type.rented }} <span class="report__metric--thin">из {{ type.total }}</span></p>
                        </div>
                        <div class="report__metric report__metric--right">
                            <p>Свободно</p>
                            <p>{{ type.available }} <span class="report__metric--thin">из {{ type.total }}</span></p>
                        </div>
                    </div>
                </div>
            {% endfor %}
        </div>
    </div>
</section>

{% endblock %}
